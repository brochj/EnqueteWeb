@model IEnumerable<Resposta>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/lib/bootstrap5/dist/css/bootstrap.css">

    <title>EqueteWeb :: Resultado Parcial</title>
</head>
<body>
    <div class="container">
        <h1>EnqueteWeb</h1>
        <h2>Resultado Parcial</h2>
        @foreach (Resposta r in Model)
        {
            <p>@(r.Nome): @(r.Sim.Value ? "Sim" : "Não")</p>
        }
        <p><a class="btn btn-primary" asp-action="Index">Voltar ao início</a></p>
    </div>
</body>
</html>